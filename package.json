{
  "name": "newsdna-webpack-config",
  "version": "1.0.0",
  "description": "Base webpack cnofig file for vanilla JS web-apps",
  "main": "webpack.config.js",
  "scripts": {
    "start": "NODE_ENV=HOT webpack-dev-server --config webpack.config.js --colors",
    "uat": "clear && NODE_ENV=UAT webpack --config webpack.config.js --progress --colors --watch",
    "prod": "clear && NODE_ENV=PROD webpack -p --config webpack.config.js --progress --colors",
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/martin-banks/newsdna-webpack-config.git"
  },
  "keywords": [],
