  "bugs": {
    "url": "https://github.com/martin-banks/newsdna-webpack-config/issues"
  },
  "homepage": "https://github.com/martin-banks/newsdna-webpack-config#readme",
  "devDependencies": {
    "babel-core": "6.13.2",
    "babel-loader": "6.2.4",
    "babel-preset-es2015": "6.13.2",
    "babel-preset-stage-2": "6.13.0",
    "babili-webpack-plugin": "0.0.11",
    "css-loader": "0.23.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "html-webpack-plugin": "^2.24.1",
    "imagemagick": "^0.1.3",
    "ip": "^1.1.5",
    "mkdirp": "^0.5.1",
    "node-sass": "^4.5.3",
